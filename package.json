{
  "name": "guildwars2",
  "version": "1.0.0",
  "description": "View your Guild Wars 2 stats and achievements",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node_modules/.bin/npm-run-all --parallel server dev",
    "mock": "cross-env NODE_ENV=mock node_modules/.bin/npm-run-all --parallel mockServer dev",
    "build": "cross-env NODE_ENV=production npm-run-all --serial clean dist",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "dist": "webpack --config ./webpack/webpack.prod.js",
    "server": "node node_modules/guildwars2-server/dist/main",
    "mockServer": "node node_modules/guildwars2-server/dist/mock",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src",
    "security": "node_modules/.bin/nsp check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrowe122/GuildWars2.git"
  },
  "author": "Steven Rowe",
  "license": "GPL-3.0",
  "homepage": "https://github.com/mrowe122/GuildWars2#readme",
  "dependencies": {
    "context-hoc": "https://github.com/mrowe122/ContextHoc",
    "date-fns": "^1.29.0",
    "emotion": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "firebase": "^5.3.1",
    "flexboxgrid": "^6.3.1",
    "formik": "^1.0.2",
    "lodash": "^4.17.10",
    "mdi-react": "^4.0.0",
    "outside-click-hoc": "https://github.com/mrowe122/OutsideClickHoc",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-router-dom": "^4.3.1",
    "react-spinkit": "^3.0.0",
    "react-tabs": "^2.2.1",
    "recompose": "^0.28.1"
  },
  "devDependencies": {
    "abortcontroller-polyfill": "^1.1.9",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "circular-dependency-plugin": "^5.0.2",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "guildwars2-server": "https://github.com/mrowe122/GuildWars2-Server",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^1.0.0-rc.13",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.1.5",
    "jest": "^23.4.1",
    "jest-fetch-mock": "^1.6.5",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "path": "^0.12.7",
    "prettier": "^1.14.0",
    "promise-polyfill": "^8.0.0",
    "react-hot-loader": "^4.3.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^2.0.4"
  }
}
